{
    "name": "atom-azimov/uploader",
    "description": "A lightweight PHP library to file uploads with ORM embeddable objects, ORM entities, etc.",
    "license": "MIT",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Atom\\Uploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Atom\\Uploader\\": "spec/"
        }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpspec/phpspec": "^2.4",
        "phpunit/phpunit": "^5.1",
        "php-vfs/php-vfs": "^1.3",
        "henrikbjorn/phpspec-code-coverage": "^2.0.2",
        "league/flysystem": "^1.0",
        "satooshi/php-coveralls": "^1.0"
    }
}
